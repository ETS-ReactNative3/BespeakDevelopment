{
  "expo": {
    "name": "Bespeak",
    "slug": "Bespeak",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/bespeak-icon-test.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/bespeak-splash-test.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/bespeak-icon-test.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.jedpedregosa.Bespeak"
    },
    "plugins": [
      "@react-native-firebase/app",
      ["expo-notifications",
        {
          "icon": "./assets/ic_notification.png"
        }
      ]
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
